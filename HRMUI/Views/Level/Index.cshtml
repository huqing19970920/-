@model IEnumerable<HRMModel.OneLevelModel>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link href="~/info/css/table.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/info/javascript/comm/comm.js"></script>
    <script src="~/Scripts/vue.js"></script>
</head>
<body>
    <form method="post" action="../customiz/customizAction!findFirstKind" id="form1">
        <table width="100%">
            <tr>
                <td>
                    <font color="black">
                        您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--I级机构设置
                    </font>
                </td>
            </tr>
            <tr>
                <td align="right">
                    <input type="button" value="添加" class="BUTTON_STYLE1"
                           onclick="window.location.href='first_kind_register.html'" />
                </td>
            </tr>
        </table>
        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <tr>
                <td width="20%" class="TD_STYLE1">
                    I级机构编号
                </td>
                <td width="20%" class="TD_STYLE1">
                    I级机构名称
                </td>
                <td width="25%" class="TD_STYLE1">
                    薪酬发放责任人编号
                </td>
                <td width="25%" class="TD_STYLE1">
                    销售责任人编号
                </td>
                <td width="5%" class="TD_STYLE1">
                    变更
                </td>
                <td width="5%" class="TD_STYLE1">
                    删除
                </td>
            </tr>
            <tr  v-for="item in dt">
                <td style="display:none">
                    {{item.Id}}
                </td>
                <td class="TD_STYLE2">
                  {{item.first_kind_id}}
                </td>
                <td class="TD_STYLE2">
                  {{item.first_kind_name}}
                </td>
                <td class="TD_STYLE2">
                    {{first_kind_salary_id}}
                </td>
                <td class="TD_STYLE2">
               {{first_kind_sale_id}}
                </td>
                <td class="TD_STYLE2">
                    <a class="btn btn-default" href="#">修改</a>
                </td>
                <td class="TD_STYLE2">
                    <a href="#" class="btn btn-warning">删除</a>
                </td>
            </tr>
        </table>
    </form>
    <script type="text/javascript">
        var vm = new Vue({
            el: "#form1",
            data: {
                dt: null
            },
            methods: {
                load: function () {
                    var t = this;
                    $.ajax({
                        url: "/Level/Selectlist",
                        type: "post",
                        data: "",
                        success: function (msg) {
                            var dd = eval("(" + msg + ")");
                            t.dt = dd;
                        }
                    });
                }
            },
            created: function () {
                this.load();
            }
        })
    </script>
</body>
</html>
